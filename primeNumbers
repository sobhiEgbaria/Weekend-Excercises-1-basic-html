const primeNumberFromOneToN = (n)=>{

    let isPrime = true, counter = 0;

for(let i=2 ; i<=n; i++){
    isPrime = true;
    for (let j = 2; j<i ; j++) {
        if (i%j == 0) {
             isPrime = false;
                 break;  }   
    }
    if (isPrime)
       counter++;
    }
    return(counter);
}
 